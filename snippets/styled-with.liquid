<h1>Hello world</h1>


<div id="" class="active">
    <div class="product-list">
        <div class="wrapper">
            <h2 class="blockHeader">Styled With</h2>
            <div class="product-grid clearfix">
              {% for styled_product in product.metafields.product.styled_with.value %}
                <div class="item" id="">
                  <div class="product-image-wrap">
                    <a class="image" href="{{ styled_product.url }}">
                      <img src="{{ styled_product.featured_image | img_url: 'master' }}" loading="lazy" importance="low" alt="Blue Cotton-Cashmere Dress">
                    </a>
                  </div>
                  <div class="product-caption">
                    <h2 class="product-name">
                      <a href="{{ styled_product.url }}">{{styled_product.title}}</a>
                    </h2>
                    <span class="product-price"><s><span class="money">{{ styled_product.compare_at_price | money }}</span></s></span>
                    <span class="product-price on-sale on-sale-margin"><span class="money">{{styled_product.price | money}}</span></span>
                  </div>
              </div>
              {% endfor %}
            </div>
          
        </div>
    </div>
</div>